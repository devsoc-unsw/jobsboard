{
  "name": "jobs-board-backend",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@t3-oss/env-core": "^0.6.1",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.1",
    "fast-levenshtein": "^3.0.0",
    "functional-red-black-tree": "^1.0.1",
    "fuse.js": "^6.6.2",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "ldapts": "^7.0.0",
    "mongodb": "^4.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.3.0",
    "openapi-types": "^12.0.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^5.0.0",
    "text-table": "^0.2.0",
    "typeorm": "^0.3.0",
    "uri-js": "^4.4.1",
    "winston": "^3.2.1",
    "zod": "^3.21.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "./tests"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --incremental",
    "start:dev": "nodemon",
    "prestart": "yarn run build",
    "dev": "nodemon -w ./dist yarn run watch",
    "serve": "nodemon -w ./src yarn run watch",
    "watch": "ts-node src/index.ts",
    "watch:test": "sleep 7 && ./node_modules/.bin/mocha --reporter mochawesome ./tests/*_tests.js",
    "test": "nodemon -w ./src -w ./tests -x yarn run watch:test",
    "travis:test": "./node_modules/.bin/mocha --reporter mochawesome ./tests/*_tests.js",
    "format": "prettier './src/**/*.ts'",
    "format:fix": "prettier --write './src/**/*.ts'",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint --fix './src/**/*.ts'"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/helmet": "0.0.48",
    "@types/node": "20.9.2",
    "@types/nodemailer": "6.4.9",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "chai": "4.3.7",
    "eslint": "8.36.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "mocha": "10.2.0",
    "mochawesome": "7.1.3",
    "nodemon": "^3.0.0",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "6.1.3",
    "typescript": "4.9.5"
  }
}
